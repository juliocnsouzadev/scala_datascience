package week3.lists

class Cons[T](val head: T, val tail: List[T]) extends List[T] {
  
  private val h : T = head
  private val t : List[T] = tail
  
  def isEmpty : Boolean = false
  
  def nth[T](n: Int): T = 
    nthInner(n- 1, this.tail) 
    
  private def  nthInner[T](n: Int, xs: List[T]): T =  
    if (n == 0) this.head
    else nthInner(n- 1, t) 
  
}