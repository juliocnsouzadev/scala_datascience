package week4.lists

object Testing {
  val fruits = List("oranges", "apples", "melons") //> fruit  : List[String] = List(oranges, apples, melons)
  val fruits = "apples" :: ("oranges" :: ("melons" ::(Nil)))
                                                  //> f  : List[String] = List(apples, oranges, melons)
  fruit.foreach { f => System.out.print(f) }
}