package week4.lists

object Testing {
  val fruits1 = List("oranges", "apples", "melons")
                                                  //> fruits1  : List[String] = List(oranges, apples, melons)
  val fruits2 = "apples" :: ("oranges" :: ("melons" ::(Nil)))
                                                  //> fruits2  : List[String] = List(apples, oranges, melons)
  fruits1
  .filter { f => f != null }
  .foreach { f => System.out.print(f) }
}